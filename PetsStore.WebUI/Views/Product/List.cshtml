@model PetsStore.WebUI.Models.ProductsListViewModel

@{
    ViewBag.Title = "Products";
}

@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary", p)
}
<div class=" mt-4 btn-group float-end">
    @Html.PageLinks(Model.PagingInfo, x => Url.Action
        ("List", new { page = x, category = Model.CurrentCategory }))
</div>

